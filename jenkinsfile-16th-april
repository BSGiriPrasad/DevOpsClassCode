pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                echo 'I am doing a git clone'
                git 'https://github.com/awsdevopsclass2022/DevOpsClassCodes-aug.git'
            }
        }

        stage('compile') {
            steps {
                echo 'Hello World'
            }
        }

        stage('test') {
            steps {
                echo 'Hello World'
            }
        }

        stage('package') {
            steps {
                echo 'Hello World'
            }
        }

        stage('docker build') {
            steps {
                echo 'Hello World'
            }
        }

        stage('docker login') {
            steps {
                echo 'Hello World'
            }
        }

        stage('docker push') {
            steps {
                echo 'Hello World'
            }
        }


    }
}